Query:
SELECT [
FunctionCall null.SQRT@1[
  OperatorExpr [
    FieldAccessor [
      FunctionCall Metadata.dataset@1[
        LiteralExpr [STRING] [t]
      ]
      Field=a
    ]
    *
    FieldAccessor [
      FunctionCall Metadata.dataset@1[
        LiteralExpr [STRING] [t]
      ]
      Field=b
    ]
  ]
]
root
]
FROM [  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [tbl_name]
  ]
  AS
  Variable [ Name=$root ]
]
Groupby
  Variable [ Name=#1 ]
  :=
  FieldAccessor [
    Variable [ Name=$root ]
    Field=id
  ]
  GROUP AS
  Variable [ Name=#2 ]
  (
  Variable [ Name=$root ]
   AS root
  )

Let Variable [ Name=$u ]
  :=
  FieldAccessor [
    Variable [ Name=$root ]
    Field=time
  ]
 HAVING
  OperatorExpr [
    FieldAccessor [
      Variable [ Name=$root ]
      Field=orders
    ]
    >
    LiteralExpr [LONG] [0]
  ]
Orderby
  Variable [ Name=$u ]
  ASC

