use dataverse recovery;

/* Load Fragile data from raw dataset into cleaned dataset */
insert into dataset Fragile (
  for $t in dataset Fragile_raw
  return {
    "row_id": $t.row_id % 28000,
    "sid": $t.sid,
    "date": date($t.date),
    "day": $t.day,
    "time": parse-time($t.time, "h:m:s"),
    "bpm": $t.bpm,
    "RR": $t.RR,
    "text": $t.text,
    "location": $t.location,
    "text2": $t.text2
  }
);